        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    // Add your code here
    var lvc = new GlideRecord('x_1292527_lva_loaner_vehicle_catalog');
    lvc.addQuery('sys_id', current.vehicle);
    lvc.query();
    if (lvc.next()) {
        if ((current.vehicle_status == 'pending_release') || (current.vehicle_status == 'out_on_field') || (current.vehicle_status == 'returned_for_inspection') || (current.vehicle_status == 'sent_for_servicing')) {
            lvc.status = 'unavailable';
        }
        else if (current.vehicle_status == 'returned_to_warehouse') {
            lvc.status = 'available';
        }
        else if (current.vehicle_status == 'decommissioned') {
            lvc.status = 'decommissioned';
        }
        lvc.update();
    }
})(current, previous);]]></script>
